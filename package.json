{
  "name": "dostoevsky-website",
  "description": "Website of Dostoevsky project",
  "version": "0.0.0",
  "author": "goooseman <goooseman@me.com>",
  "private": true,
  "dependencies": {
    "gatsby": "^2.23.7",
    "gatsby-image": "^2.4.7",
    "gatsby-link": "^2.4.6",
    "gatsby-plugin-catch-links": "^2.3.5",
    "gatsby-plugin-google-analytics": "^2.3.4",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.4.12",
    "gatsby-plugin-netlify": "^2.3.5",
    "gatsby-plugin-offline": "^3.2.11",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sass": "^2.2.4",
    "gatsby-plugin-sharp": "^2.6.12",
    "gatsby-plugin-sitemap": "^2.4.5",
    "gatsby-plugin-twitter": "^2.3.4",
    "gatsby-plugin-typescript": "^2.4.4",
    "gatsby-remark-copy-linked-files": "^2.3.5",
    "gatsby-remark-images": "^3.3.11",
    "gatsby-remark-prismjs": "^3.5.4",
    "gatsby-remark-responsive-iframe": "^2.4.5",
    "gatsby-remark-smartypants": "^2.3.4",
    "gatsby-source-filesystem": "^2.3.12",
    "gatsby-transformer-remark": "^2.8.17",
    "gatsby-transformer-sharp": "^2.5.5"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "animate.css": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.5.0",
    "emergence.js": "^1.1.2",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "font-awesome": "^4.7.0",
    "gh-pages": "^3.0.0",
    "husky": "^4.2.5",
    "jquery": "^3.5.1",
    "lint-staged": "^10.2.11",
    "modern-normalize": "^0.6.0",
    "node-sass": "^4.14.1",
    "popper.js": "^1.16.1",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "textlint": "^11.6.3",
    "textlint-rule-preset-ja-spacing": "^2.0.1",
    "textlint-rule-preset-japanese": "^4.0.4",
    "typescript": "^3.9.3"
  },
  "keywords": [
    "dostoevsky"
  ],
  "license": "",
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [
      "prettier --write"
    ],
    "*.md": [
      "npm run lint:textfix"
    ]
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\" --ignore-path .gitignore",
    "lint": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "prettier --write \"**/*.+(js|jsx|json|css|scss)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
